{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "strict": true,
    "noImplicitThis": false,
    "outDir": "../../dist/out-tsc",
    "types": ["vue-i18n", "node"],
    "target": "ESNext",
    "declaration": true
  },
  "exclude": ["**/*.spec.ts"],
  "include": [
    "**/*.js",
    "**/*.ts",
    "**/*.vue",
    "**/*.json",
    "../../tsconfig.base.json",
    "../../libs/**/*.js",
    "../../libs/**/*.ts"
  ]
}
