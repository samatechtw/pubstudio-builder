{
  "name": "pubstudio-builder",
  "version": "1.0.0",
  "description": "Page editor, renderer, and backend that powers https://pubstud.io",
  "engines": {
    "node": ">=14.17.4",
    "npm": ">=7.17.0"
  },
  "scripts": {
    "format": "nx format:write",
    "format:check": "nx format:check",
    "lint": "nx workspace-lint && nx lint",
    "lint:style": "./node_modules/stylelint/bin/stylelint.js \"**/src/**/*.{css,vue}\"",
    "build:builder-demo": "cd apps/builder-demo && vite build --outDir ../../dist/apps/builder-demo --emptyOutDir",
    "build:web-site": "cd apps/web-site && vite build --outDir ../../dist/apps/web-site --emptyOutDir",
    "dev:build-aarch": "cd backend && CARGO_TARGET_AARCH64_UNKNOWN_LINUX_GNU_LINKER=aarch64-unknown-linux-gnu-gcc cargo build --target aarch64-unknown-linux-gnu",
    "dev:run-aarch": "cd backend && CARGO_TARGET_AARCH64_UNKNOWN_LINUX_GNU_LINKER=aarch64-unknown-linux-gnu-gcc cargo run --target aarch64-unknown-linux-gnu",
    "dev:build": "cd backend && cargo build",
    "dev:run": "cd backend && cargo run",
    "dev:watch": "cd backend && cargo watch --why -x run",
    "dev:watch-aarch": "cd backend && CARGO_TARGET_AARCH64_UNKNOWN_LINUX_GNU_LINKER=aarch64-unknown-linux-gnu-gcc cargo watch --why -- bash -c \"(cargo build --target aarch64-unknown-linux-gnu)\"",
    "prod:build": "cd backend && CARGO_TARGET_AARCH64_UNKNOWN_LINUX_GNU_LINKER=aarch64-unknown-linux-gnu-gcc cargo build --release --target aarch64-unknown-linux-gnu",
    "ci:build": "cd backend && cargo build --release",
    "prune": "tools/scripts/prune-all.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samatechtw/pubstudio-builder.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samatechtw/pubstudio-builder/issues"
  },
  "homepage": "https://github.com/samatechtw/pubstudio-builder#README",
  "author": "SamaTech",
  "dependencies": {
    "@floating-ui/vue": "^1.0.2",
    "@samatech/vue-store": "^0.5.1",
    "@sampullman/fetch-api": "^0.11.3",
    "@unhead/vue": "^1.7.4",
    "@vuepic/vue-datepicker": "7.2.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "jwt-decode": "^3.1.2",
    "prosemirror-commands": "^1.5.2",
    "prosemirror-gapcursor": "^1.3.2",
    "prosemirror-history": "^1.3.2",
    "prosemirror-inputrules": "^1.2.1",
    "prosemirror-keymap": "^1.2.2",
    "prosemirror-model": "^1.19.3",
    "prosemirror-schema-list": "^1.3.0",
    "prosemirror-state": "^1.4.3",
    "prosemirror-transform": "^1.8.0",
    "prosemirror-view": "^1.32.1",
    "vue": "^3.3.6",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/config-angular": "^17.8.0",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@jest/globals": "^29.7.0",
    "@nx/eslint-plugin": "16.10.0",
    "@nx/jest": "16.10.0",
    "@nx/js": "16.10.0",
    "@nx/linter": "16.10.0",
    "@nx/workspace": "16.10.0",
    "@playwright/test": "^1.39.0",
    "@samatech/image-api-types": "^0.0.2",
    "@samatech/postcss-basics": "^0.6.0",
    "@types/convict": "^6.1.4",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^18.16.8",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@videojs/themes": "^1.0.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "commitlint": "^17.8.0",
    "convict": "^6.2.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "jest": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "jsonwebtoken": "^9.0.2",
    "nx": "16.10.0",
    "nx-vue3-vite": "0.26.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "4.4.11",
    "vite-plugin-singlefile": "^0.13.5",
    "vue-eslint-parser": "^9.3.2"
  }
}
